{
	"suggest.completionItemKindLabels": {
		"keyword":       "\uf1de",
		"variable":      "\ue79b",
		"value":         "\uf89f",
		"operator":      "\u03a8",
		"function":      "\u0192",
		"reference":     "\ufa46",
		"constant":      "\uf8fe",
		"method":        "\uf09a",
		"struct":        "\ufb44",
		"class":         "\uf0e8",
		"interface":     "\uf417",
		"text":          "\ue612",
		"enum":          "\uf435",
		"enumMember":    "\uf02b",
		"module":        "\uf40d",
		"color":         "\ue22b",
		"property":      "\ue60b",
		"field":         "\uf9be",
		"unit":          "\uf475",
		"event":         "\ufacd",
		"file":          "\uf723",
		"folder":        "\uf114",
		"snippet":       "\ue60b",
		"typeParameter": "\uf728",
		"default":       "\uf29c"
	},
	"languageserver": {
		"lua": {
			"command":   "lua-lsp",
			"filetypes": ["lua"]
		},
		"ccls": {
			"command":      "ccls",
			"filetypes":    ["c", "cpp", "objc", "objcpp"],
			"rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
			"initializationOptions": {
				"cache": {
					"directory": "/tmp/ccls"
				},
				"highlight": { "lsRanges" : true },
				"client": {
					"snippetSupport": true
				}
			}
		},
		"js":
		{
			"command": "javascript-typescript-stdio",
			"filetypes": ["javascript", "typescript"],
			"rootPatterns": [".git/"]
		},
		"dockerfile": {
			"command":   "docker-langserver",
			"filetypes": ["dockerfile"],
			"args":      ["--stdio"]
		},
		"bash": {
			"command":          "bash-language-server",
			"args":             ["start"],
			"filetypes":        ["sh", "PKGBUILD"],
			"ignoredRootPaths": ["~"],
			"trace.server":     "verbose"
		},
		"cmake": {
			"command":               "cmake-language-server",
			"filetypes":             ["cmake"],
			"rootPatterns":          ["build/"],
			"initializationOptions": {
				"buildDirectory": "build"
			},
			"trace.server":          "verbose"
		},
		"svls": {
			"command": "svls",
			"filetypes": ["systemverilog", "verilog"]
		},
		"sql": {
			"command": "sqls",
			"args": ["-config", "$HOME/.config/sqls/config.yml"],
			"filetypes": ["sql"],
			"shell": true
		},
		"omnisharp": {
			"command": "~/.cache/omnisharp-vim/omnisharp-roslyn/OmniSharp.exe",
			"args": [
				"-lsp"
			],
			"filetypes": ["cs"],
			"rootPatterns": [".git/"],
			"trace.server": "verbose"
		},
		"digestif": {
			"command": "digestif",
			"filetypes": ["tex", "plaintex", "context"]
		}
	},
	"diagnostic.virtualText":                true,
	"diagnostic.virtualTextCurrentLineOnly": false,
	"diagnostic.warningSign":                " ",
	"codeLens.enable":                       true,
	"suggest.enablePreview":                 true,
	"signature.target":                      "float",
	"codeLens.separator":                    "⇒",
	"json.trace.server":                     "verbose",
	"coc.preferences.highlightTimeout":      1000,
	"coc.preferences.enableMessageDialog":   true,
	"git.showCommitInFloating": true,
	"coc.preferences.formatOnSaveFiletypes": ["typescript"]
}
